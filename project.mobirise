\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <h3 class=\"mbr-section-title align-center mb-5 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .social-list\">\n                <b>Join us on social media</b></h3>\n            <div class=\"col-12\">\n                <div class=\"social-row\" mbr-if=\"showSoc\">\n                    <div class=\"soc-item\">\n                        \n                            <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\" style=\"font-size: 35px;\"></span>\n                        \n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"soc > 1\">\n                        \n                            <span mbr-icon class=\"mbr-iconfont socicon-twitter socicon\" style=\"font-size: 35px;\"></span>\n                        \n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"soc > 2\">\n                        \n                            <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\" style=\"font-size: 35px;\"></span>\n                        \n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"soc > 3\">\n                        \n                            <span mbr-icon class=\"mbr-iconfont socicon-tiktok socicon\" style=\"font-size: 35px;\"></span>\n                        \n                    </div>\n                    <div class=\"soc-item\" mbr-if=\"soc > 4\">\n                        \n                            <span mbr-icon class=\"mbr-iconfont socicon-tiktok socicon\"></span>\n                        \n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
